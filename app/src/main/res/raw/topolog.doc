#include "BoolMatrix.h"
#include "BoolVector.h"
#include <iostream>
using namespace std;
int main()
{
    setlocale(LC_ALL, "RU");
    int n = 9, i = 0;
    char** s;
    s = new char* [n];
    for (int i = 0; i < n; i++)
    {
        s[i] = new char[n+1];
    }
    strcpy_s(s[0], 10, "000101000");///////////0
    strcpy_s(s[1], 10, "101000100");
    strcpy_s(s[2], 10, "000100000");
    strcpy_s(s[3], 10, "000000000");
    strcpy_s(s[4], 10, "001000000");
    strcpy_s(s[5], 10, "000010000");
    strcpy_s(s[6], 10, "000000010");
    strcpy_s(s[7], 10, "000010000");
    strcpy_s(s[8], 10, "000111000");
    BoolMatrix  M(s, n);
    BoolVector v0(n), v1(n), d(n), w(n);
    int* res;
    res = new int[9];
    cout << M;
    cout << endl;
    while (i < n)
    {
        d = M.Disj();
        w = ~d;
        v1 = v0 ^ w;

        for (int j = 0; j < n; j++)
        {
            if (v1[n - j - 1])
            {
                res[i] = j + 1;
                i++;
                M.SetUp0(j, 0, n);
            }
        }
        v0 = w;
    }
    cout << "Ответ: ";
    for (int i = 0; i < n; i++)
    {
        cout << res[i] << " ";
    }
    for (int i = 0; i < 9; i++) {
        delete[] s[i];
    }
    delete[] s;
    delete[] res;
    return 0;
}